git clone --recursive https://github.com/riscv/riscv-gnu-toolchain

cd riscv-gnu-toolchain

./configure --prefix=/opt/riscv-elf-gcc
sudo make

./configure --prefix=/opt/riscv-elf-gcc
sudo make linux
sudo make report-linux

./configure --prefix=/opt/riscv-elf-gcc --enable-multilib
sudo make linux
sudo make report-linux
