mkdir sim/mixed/regression/run
mkdir sim/verilog/regression/run

ln -s $PWD/sim/mixed/regression/bin/Makefile         $PWD/sim/mixed/regression/run
ln -s $PWD/sim/mixed/regression/bin/Makefile.include $PWD/sim/mixed/regression/run

ln -s $PWD/sim/verilog/regression/bin/Makefile         $PWD/sim/verilog/regression/run
ln -s $PWD/sim/verilog/regression/bin/Makefile.include $PWD/sim/verilog/regression/run


rm -rf rtl/verilog/pkg/riscv_mpsoc_pkg.sv
rm -rf rtl/vhdl/pkg/riscv_mpsoc_pkg.vhd

cp ../rtl/verilog/pkg/riscv_mpsoc_pkg.sv rtl/verilog/pkg
cp ../rtl/vhdl/pkg/riscv_mpsoc_pkg.vhd   rtl/vhdl/pkg
